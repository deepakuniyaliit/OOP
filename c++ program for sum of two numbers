#include <iostream>
int main(void)
{
    int firstNumber{};
	int secondNumber{};
	int sumOfTwoNumbers{};//for hacktober fest

    std::cout << "Enter two integers: ";
    std::cin >> firstNumber >> secondNumber;

    sumOfTwoNumbers = firstNumber + secondNumber;

    std::cout << firstNumber << " + " << secondNumber << " = " << sumOfTwoNumbers;

    return 0;
}
